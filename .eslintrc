{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "prettier"
    ],
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "ignorePatterns": ["build"],
    "rules": {
        "no-console": "error",
        "no-unused-vars": "off"
    }
}
